!! Dracula Xresources palette
*.foreground: #F8F8F2
*.background: [95]#282A36
!! black
*.color0: #000000
*.color8: #4D4D4D
!! red
*.color1: #FF5555
*.color9: #FF6E67
!! green
*.color2: #50FA7B
*.color10: #5AF78E
!! yellow
*.color3: #F1FA8C
*.color11: #F4F99D
!! blue
*.color4: #BD93F9
*.color12: #CAA9FA
!! magenta
*.color5: #FF79C6
*.color13: #FF92D0
!! cyan
*.color6: #8BE9FD
*.color14: #9AEDFE
!! white
*.color7: #BFBFBF
*.color15: #E6E6E6

URxvt.intensityStyles: false
URxvt.depth: 32
URxvt.font: xft:DejaVu Sans Mono:size=12
URxvt.scrollBar: false
URxvt.cursorColor: #50FA7B

!! Cycle commands
URxvt.keysym.M-k: \033[A
URxvt.keysym.M-j: \033[B

!! jump between words
URxvt.keysym.M-h: \033[1;5D
URxvt.keysym.M-l: \033[1;5C

!! extensions
URxvt.perl-ext-common: default,url-select,matcher,keyboard-select,selection-to-clipboard
URxvt.colorUL: #FF79C6

!! url-select
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.launcher: webview
URxvt.url-select.underline: true

!! keyboard-select
URxvt.keysym.M-p: perl:keyboard-select:activate

!! matcher
URxvt.url-launcher: webview
URxvt.matcher.button: 1
